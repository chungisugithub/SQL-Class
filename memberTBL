drop table memberTBL;
CREATE TABLE memberTBL -- 회원 테이블
( memberID  	CHAR(8) NOT NULL PRIMARY KEY, -- 사용자 아이디(PK)
  memberName  	NCHAR(8) NOT NULL, -- 이름
  memberAddress 	NVARCHAR2(20) 
);
